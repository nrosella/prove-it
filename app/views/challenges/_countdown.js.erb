$(document).ready(function(){
  var finishTime = new Date(); 
  finishTime = new Date("<%= @challenge.challenge_end %>"); 
  $('#defaultCountdown').countdown({until: finishTime, onExpiry: function(){
    $.ajax({
      url: "<%= new_vote_path %>", 
      dataType: "script", 
      type: "GET",
      data: {id: "<%= @challenge.id %>"}});
    },
  });
  $('#votingCountdown').hide();
});